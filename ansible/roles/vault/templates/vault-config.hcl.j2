ui = true
disable_mlock = true

storage "s3" {
  bucket = "{{ vault_s3_bucket }}"
  region = "{{ aws_region }}"
}

listener "tcp" {
  address     = "0.0.0.0:8200"
  tls_disable = 1
}

seal "awskms" {
  region     = "{{ aws_region }}"
  kms_key_id = "{{ vault_kms_key_id }}"
}

api_addr = "http://{{ ansible_default_ipv4.address }}:8200"
cluster_addr = "http://{{ ansible_default_ipv4.address }}:8201"
