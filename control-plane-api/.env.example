# Control Plane API Configuration Example
# Copy this file to .env and fill in your values

# Application
APP_NAME="APIM Control Plane API"
ENVIRONMENT=dev
DEBUG=false

# webMethods
WEBMETHODS_URL=https://gateway.apim-dev.votredomaine.com
WEBMETHODS_USERNAME=Administrator
WEBMETHODS_PASSWORD=<get-from-vault>

# AWS Cognito
COGNITO_REGION=eu-west-1
COGNITO_USER_POOL_ID=eu-west-1_XXXXX
COGNITO_CLIENT_ID=<your-client-id>

# AWS
AWS_REGION=eu-west-1
DYNAMODB_TENANTS_TABLE=apim-tenants-dev
DYNAMODB_APIS_TABLE=apim-apis-metadata-dev

# Logging
LOG_LEVEL=INFO
