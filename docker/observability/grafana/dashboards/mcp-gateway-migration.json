        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Python"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Rust"
            },
            "properties": [
              {
                "id": "color",
